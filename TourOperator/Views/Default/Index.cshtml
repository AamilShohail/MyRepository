@model IEnumerable<TourOperator.Models.Default>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DefaultPackages)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PackageType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TourFare)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DefaultPackages)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PackageType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TourFare)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PackageID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PackageID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PackageID })
        </td>
    </tr>
}

</table>
